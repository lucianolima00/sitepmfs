<!doctype html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="ifg test site">
  <meta name="author" content="Luciano Lima">
  <meta name="keywords" content="ifg admin test">
	<!-- Font Awesome -->
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css">
	<!-- Bootstrap core CSS -->
	<link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet">
	<!-- Material Design Bootstrap -->
	<link href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.6.1/css/mdb.min.css" rel="stylesheet">
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css" integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS" crossorigin="anonymous">
	<link href="https://fonts.googleapis.com/css?family=Lato|Montserrat|Raleway|Staatliches" rel="stylesheet">
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <title>PMFS</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
</head>
  <body id="body">
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>

    <!-- NavBar -->
    <nav id="navbar" class="wrapper">
      <div>
        <p class="pg-title">BCCTest</p>
      </div>
      <div>
        <div id="navbar-logo">
          <a  class="navbar-brand" href="#">
            <img src="img/icons/logo.png" width="52" height="45" alt="">
          </a>
        </div>
      </div>
      <div>
        <a id="navbar-account" href="account.html">
          <span>Luciano</span>
          <img class="acc-pict" src="img/icons/profilepictures/profilepicture.jpg" width="35" height="35" alt="">
        </a>
      </div>
    </nav>
    <!-- NavBar Ends -->
    <!-- Aside -->
    <div class="float-left" id="aside-nav">
      <ul class="nav flex-column h-100">
        <li class="aside-item">
          <div class="nav-link">
            <i class="fas fa-search"></i><input class="form-control form-control-sm aside-span" id="aside-input" placeholder="Search...">
          </div>
        </li>
        <li class="aside-item">
          <a class="nav-link" href="dashboard-admin.html">
            <i class="fas fa-tachometer-alt"></i><span class="aside-span">Dashboard</span>
          </a>
        </li>
        <li class="aside-item">
          <%=link_to questionnaires_path, class: "nav-link" do%>
            <i class="fas fa-file-alt" style="padding-left: 3.5px; margin-right: 1px;"></i><span class="aside-span">Tests</span>
          <% end %>
        </li>
        <li class="aside-item">
          <a class="nav-link" href="users.html">
            <i class="fas fa-users"></i><span class="aside-span">Users</span>
          </a>
        </li>
        <li class="aside-item" id="cfg-btn" >
          <a class="nav-link" href="#">
            <i class="fas fa-cog"></i><span class="aside-span">Options</span>
          </a>
        </li>
      </ul>
    </div>
    <!-- Aside Ends-->

    <div class="container">
      <!-- Content -->
      <div class="content">
        <%= yield %>
      </div>
      <!-- Contents Ends -->
    </div>
    <!-- Container Ends -->
    <!-- JQuery -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <!-- Bootstrap tooltips -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.4/umd/popper.min.js"></script>
    <!-- Bootstrap core JavaScript -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/js/bootstrap.min.js"></script>
    <!-- MDB core JavaScript -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.6.1/js/mdb.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.min.js" integrity="sha384-B0UglyR+jN6CkvvICOB2joaf5I4l3gm9GU6Hc1og6Ls7i6U/mkkaduKaBhlAXv9k" crossorigin="anonymous">
    </script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <script
    src="https://code.jquery.com/jquery-3.3.1.slim.js"
    integrity="sha256-fNXJFIlca05BIO2Y5zh1xrShK3ME+/lYZ0j+ChxX2DA="
    crossorigin="anonymous"></script>
    <script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>  
  </body>
</html>