<p id="notice"><%= notice %></p>

<h1>Questions</h1>

<table>
  <thead>
    <tr>
      <th>Textstatement</th>
      <th>Alta</th>
      <th>Altb</th>
      <th>Altc</th>
      <th>Altd</th>
      <th>Alte</th>
      <th>Correctalt</th>
      <th>Questionnaire</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= question.textStatement %></td>
        <td><%= question.altA %></td>
        <td><%= question.altB %></td>
        <td><%= question.altC %></td>
        <td><%= question.altD %></td>
        <td><%= question.altE %></td>
        <td><%= question.correctAlt %></td>
        <td><%= question.questionnaire %></td>
        <td><%= link_to 'Show', question %></td>
        <td><%= link_to 'Edit', edit_question_path(question) %></td>
        <td><%= link_to 'Destroy', question, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Question', new_question_path %>
